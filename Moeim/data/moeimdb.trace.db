a-- 6: 독서\000d\000a(6, '한 달에 한 권 읽기', '부담 없이 한 달에 딱 한 권만 읽고 느낀 점 나눠요.', 7, 10, NOW(), NOW()),\000d\000a(6, '인문학 깊이 읽기', '철학, 역사 등 인문학 서적을 읽고 깊이 있는 토론을 합니다.', 4, 6, NOW(), NOW()),\000d\000a(6, '미스터리/추리 소설 매니아', '히가시노 게이고, 애거서 크리스티 등 추리 소설 좋아하시는 분!', 3, 5, NOW(), NOW()),\000d\000a\000d\000a-- 7: 여행\000d\000a(7, '제주도 게하 스탭 정보 공유', '제주도 살이, 게스트하우스 스탭 경험 나누고 정보 공유해요.', 18, 50, NOW(), NOW()),\000d\000a(7, '이번 주말 강릉 당일치기', 'KTX 타고 가서 바다 보고 커피 마시고 오실 분 구합니다.', 2, 4, NOW(), NOW()),\000d\000a(7, '유럽 배낭여행 동행 (7월)', '7월 중순 파리, 스위스 일정 겹치시는 분 밥이라도 같이 먹어요.', 1, 4, NOW(), NOW()),\000d\000a\000d\000a-- 8: 게임\000d\000a(8, '롤(LoL) 자랭 즐겜 유저', '티어 상관없습니다. 욕설 금지. 매너 게임 하실 분만.', 3, 5, NOW(), NOW()),\000d\000a(8, '배틀그라운드 스쿼드 모집', '저녁 8시~11시 고정으로 하실 분. 디스코드 필수.', 2, 4, NOW(), NOW()),\000d\000a(8, '스팀 게임 할인 정보 공유', '할인할 때 같이 사고 멀티 게임도 같이 해요.', 45, 99, NOW(), NOW()),\000d\000a\000d\000a-- 9: 기타\000d\000a(9, '반려견 산책 친구 (소형견)', '우리 강아지 사회성 길러주고 싶은 견주님들 함께 산책해요.', 4, 8, NOW(), NOW()),\000d\000a(9, '미국 주식 투자 공부방', '뉴스 공유하고 서로 종목 분석 공유합니다. 리딩방 아님.', 10, 20, NOW(), NOW()),\000d\000a(9, '동네 마실 친구 구함', '심심할 때 편의점 커피 한잔하거나 다이소 구경 가실 동네 친구.', 1, 2, NOW(), NOW()),\000d\000a(9, '미라클 모닝 챌린지', '새벽 5시 기상 인증하고 하루 목표 공유하기.', 11, 15, NOW(), NOW()),\000d\000a(9, '영화 관람 소모임', '개봉하는 영화 같이 보고 카페에서 이야기 나눠요.', 3, 4, NOW(), NOW())"; SQL statement:
